{
  "dockerfilePath": "Dockerfile",
  "buildDir": "",
  "containerPort": 8080,
  "envParams": {
    "MYSQL_ADDRESS": "${MYSQL_ADDRESS}",
    "MYSQL_USERNAME": "${MYSQL_USERNAME}",
    "MYSQL_PASSWORD": "${MYSQL_PASSWORD}",
    "REDIS_ADDRESS": "${REDIS_ADDRESS}",
    "REDIS_PASSWORD": "${REDIS_PASSWORD}",
    "PORT": "8080"
  },
  "cpu": 1,
  "mem": 2,
  "policyType": "cpu",
  "policyThreshold": 60,
  "customLogs": "stdout",
  "initialDelaySeconds": 120,
  "minNum": 1,
  "maxNum": 50,
  "remark": "智慧地产房产小程序服务端",
  "healthCheckConfig": {
    "livenessConfig": {
      "path": "/api/health",
      "port": 8080,
      "initialDelaySeconds": 120,
      "timeoutSeconds": 10,
      "periodSeconds": 30
    },
    "readinessConfig": {
      "path": "/api/health",
      "port": 8080,
      "initialDelaySeconds": 90,
      "timeoutSeconds": 10,
      "periodSeconds": 30
    }
  }
}