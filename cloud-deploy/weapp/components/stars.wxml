<view class="mystars mystars-{{ big ? 'big' : 'default' }}">
  <block   wx:for="{{[1,2,3,4,5]}}" wx:for-item="score">
    <view data-value="{{score}}" bindtap="scoreClick">
        <image wx:if="{{value >= score }}" src="/assets/icons/star-yellow.png"></image>
        <image wx:if="{{value < score }}" src="/assets/icons/star-dark.png"></image>
      </view>
    </block>
</view>