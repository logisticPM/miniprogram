.mystars{
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: center;
  width: calc(31rpx*5 + 10rpx);
}

.mystars image{
    width:31rpx;
   height: 31rpx;
}

.mystars-big{
  width: calc(50rpx*5 + 10rpx);
}
.mystars-big image{
  width: 50rpx;
  height: 50rpx;
}